{
  "name": "sietch",
  "version": "0.0.0",
  "license": "Unlicense",
  "scripts": {
    "bundle": "esbuild src/cli.ts src/index.ts --outdir=dist --bundle --platform=node --format=cjs --external:preact --external:esbuild --external:shiki --external:chokidar --external:ws --external:preact-render-to-string",
    "types": "tsc --declaration --emitDeclarationOnly --stripInternal",
    "build": "yarn bundle && yarn types",
    "watch": "yarn bundle --watch"
  },
  "main": "dist/index.js",
  "bin": {
    "sietch": "dist/cli.js"
  },
  "site": {
    "url": "https://sietch.netlify.app",
    "pagesDir": "./docs"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "esbuild": "^0.15.12",
    "mrmime": "^1.0.1",
    "picocolors": "^1.0.0",
    "preact-render-to-string": "^5.2.5",
    "rehype": "^12.0.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^2.0.1",
    "rehype-pretty-code": "^0.4.0",
    "rehype-raw": "^6.1.1",
    "rehype-slug": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "shiki": "^0.11.1",
    "to-vfile": "^7.2.3",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.1",
    "vfile-matter": "^4.0.0",
    "ws": "^8.9.0"
  },
  "peerDependencies": {
    "preact": ">=10"
  },
  "devDependencies": {
    "@types/node": "^18.11.0",
    "@types/ws": "^8.5.3",
    "preact": "^10.11.2",
    "typescript": "^4.8.4"
  }
}
